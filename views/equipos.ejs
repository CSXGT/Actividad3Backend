<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Equipos</title>
</head>
<body>
    <h1>Equipos</h1>
        <table>
            <tr>
                <th ><h3>ID_EQUIPO</h3></th>
                <th ><h3>NOMBRE</h3></th>
                <th ><h3>PATROCINANTE_ID</h3></th>
                 </tr>
            <% equipos.forEach(r => { %>
                <tr>
                    <td><%= r.ID_EQUIPO  %></td>
                    <td><%= r.NOMBRE  %></td>
                    <td><%= r.PATROCINANTE_ID  %></td>
                </tr>
            <% }) %>
        </table>

</body>
</html>