<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Modalidad</title>
</head>
<body>
    <h1>Modalidad</h1>
        <table>
            <tr>
                <th ><h3>ID_MODALIDAD</h3></th>
                <th ><h3>NOMBRE</h3></th>
                 </tr>
            <% modalidad.forEach(r => { %>
                <tr>
                    <td><%= r.ID_MODALIDAD  %></td>
                    <td><%= r.NOMBRE  %></td>
                </tr>
            <% }) %>
        </table>

</body>
</html>